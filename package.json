{
  "name": "es6-polyfills",
  "description": "ES6 polyfills that use native implementation if available and do not pollute the global namespace",
  "author": {
    "name": "The National Library of Finland"
  },
  "keywords": [
    "es6",
    "polyfill",
    "shim",
    "ecmascript",
    "harmony",
    "object",
    "assign",
    "promise"
  ],
  "homepage": "https://github.com/NatLibFi/es6-polyfills",
  "bugs": {
    "url": "https://github.com/NatLibFi/es6-polyfills/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:NatLibFi/es6-polyfills.git"
  },
  "license": " GPL-3.0",
  "version": "1.0.1",
  "scripts": {
    "lint": "jshint .",
    "test-nodejs": "istanbul cover _mocha -- test/*.spec.js",
    "test-browser": "npm run generate-requirejs-config && karma start",
    "test": "npm run test-nodejs && npm run test-browser",
    "cover": "npm run test && istanbul check-coverage",
    "check": "npm run lint && npm run cover",
    "travisci": "npm run check",
    "generate-requirejs-config": "requirejs-configurator --npm --include-dev-deps > test/browser-main.js",
    "publish-to-npm": "npm run check && npm publish"
  },
  "dependencies": {
    "es6-object-assign": "^1.0.3",
    "es6-promise-polyfill": "^1.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "jshint": "^2.9.3",
    "mocha": "^3.0.2",
    "istanbul": "^0.4.5",
    "karma": "^1.2.0",
    "karma-mocha": "^1.1.1",
    "karma-requirejs": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-requirejs-preprocessor": "^0.0.2",
    "requirejs-configurator": "https://github.com/natlibfi-arlehiko/requirejs-configurator#dev-deps",
    "codeclimate-test-reporter": "^0.3.3"
  }
}
